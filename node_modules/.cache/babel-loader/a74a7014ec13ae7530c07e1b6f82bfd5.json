A,GAAA,CAAA,GAAA;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;AACA;AACA,OALA;AAMA,KAnBA;;AAoBA;AACA,IAAA,QArBA,oBAqBA,KArBA,EAqBA,GArBA,EAqBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAAA,EAAA,GAAA,GAAA,CAAA,EAAA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,WAAA,cAAA,CAAA,QAAA,GAAA,GAAA,CAAA,QAAA;AACA,WAAA,cAAA,CAAA,KAAA,GAAA,GAAA,CAAA,KAAA;AACA,WAAA,cAAA,CAAA,MAAA,GAAA,GAAA,CAAA,MAAA;AACA,KA5BA;;AA6BA;AACA,IAAA,UA9BA,wBA8BA;AAAA;;AACA,WAAA,QAAA,CAAA,QAAA,CAAA,KAAA,cAAA,EAAA,KAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,OAAA;AACA;AACA,OANA;AAOA,WAAA,iBAAA,GAAA,KAAA;AACA,KAvCA;;AAwCA;AACA,IAAA,OAzCA,qBAyCA;AAAA;;AACA,WAAA,QAAA,CAAA,QAAA,CAAA,KAAA,QAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA;AACA,OALA;AAMA,KAhDA;;AAiDA;AACA,IAAA,UAlDA,sBAkDA,KAlDA,EAkDA,GAlDA,EAkDA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAAA,QAAA,CACA,UADA,CACA;AACA,QAAA,EAAA,EAAA,GAAA,CAAA;AADA,OADA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,OAAA;AACA;AACA,OAVA;AAWA,KA/DA;;AAgEA;AACA,IAAA,OAjEA,qBAiEA;AAAA;;AACA,WAAA,iBAAA,GAAA,KAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,WAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,KAAA,WAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,OAAA;AACA;AACA,OANA;AAOA,KA5EA;;AA6EA;AACA,IAAA,MA9EA,oBA8EA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA;;AACA,UAAA,KAAA,QAAA,CAAA,KAAA,EAAA;AACA,aAAA,QAAA,CAAA,UAAA,CAAA;AAAA,UAAA,EAAA,EAAA,KAAA,QAAA,CAAA;AAAA,SAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,gBAAA,GAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,YAAA,MAAA,CAAA,SAAA,GAAA,GAAA;AACA;AACA,SANA;AAOA,OARA,MAQA;AACA,aAAA,OAAA;AACA;AACA,KA3FA;AA4FA,IAAA,YA5FA,wBA4FA,KA5FA,EA4FA,GA5FA,EA4FA;AACA,WAAA,WAAA,GAAA,GAAA,CAAA,QAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,EAAA,GAAA,GAAA,CAAA,EAAA;AACA,KAhGA;AAiGA,IAAA,cAjGA,4BAiGA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA,CAAA,KAAA;AACA,WAAA,QAAA,CAAA,QAAA,CAAA,KAAA,EAAA,EAAA,KAAA,QAAA,CAAA,KAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA;AACA,OALA;AAMA,WAAA,aAAA,GAAA,KAAA;AACA;AA1GA,GA1EA;AAsLA,EAAA,OAtLA,qBAsLA;AAAA;;AACA,SAAA,OAAA;AACA,SAAA,QAAA,CAAA,QAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,UAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA;AACA,KANA;AAOA;AA/LA,CAAA","sourcesContent":["<!--\n * @description: \n * @version: \n * @Author: \n * @Date: 2021-01-02 10:02:56\n * @LastEditors: Please set LastEditors\n * @LastEditTime: 2021-01-02 17:47:53\n-->\n<template>\n  <div id=\"users\">\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\" class=\"breadcrumb\">\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n      <el-breadcrumb-item>用户管理</el-breadcrumb-item>\n      <el-breadcrumb-item>用户列表</el-breadcrumb-item>\n    </el-breadcrumb>\n    <el-row>\n      <el-col :span=\"12\">\n        <div class=\"grid-content bg-purple\">\n          <el-input\n            placeholder=\"请输入内容\"\n            v-model=\"userData.query\"\n            class=\"input-with-select\"\n          >\n            <el-button\n              slot=\"append\"\n              @click.native=\"search\"\n              icon=\"el-icon-search\"\n            ></el-button>\n          </el-input>\n        </div>\n      </el-col>\n      <el-col :span=\"12\">\n        <div class=\"grid-content bg-purple-light\">\n          <el-button type=\"success\" plain @click=\"dialogFormVisible = true\"\n            >添加用户</el-button\n          >\n        </div>\n      </el-col>\n    </el-row>\n    <el-dialog title=\"添加用户\" :visible.sync=\"dialogFormVisible\"  center>\n      <el-form :model=\"newUserData\" label-position=\"top\" ref=\"ruleForm\" class=\"demo-ruleForme\" :rules=\"rules\">\n        <el-form-item label=\"用户名\"  prop=\"username\" :label-width=\"formLabelWidth\">\n          <el-input\n            v-model=\"newUserData.username\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"密码\" prop=\"password\" :label-width=\"formLabelWidth\">\n          <el-input\n            v-model=\"newUserData.password\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"email\" prop=\"email\" :label-width=\"formLabelWidth\">\n          <el-input\n            v-model=\"newUserData.email\"\n            type=\"email\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"mobile\" prop=\"mobile\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"newUserData.mobile\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n        <el-button\n          type=\"primary\"\n          @click=\"addUser\"\n          >确 定</el-button\n        >\n      </div>\n    </el-dialog>\n    <el-table :data=\"tableData\" border style=\"width: 100%\">\n      <el-table-column type=\"index\" label=\"#\" width=\"auto\"> </el-table-column>\n      <el-table-column prop=\"username\" label=\"姓名\" width=\"180\"> </el-table-column>\n      <el-table-column prop=\"email\" label=\"邮箱\"> </el-table-column>\n      <el-table-column prop=\"status\" label=\"用户状态\">\n        <template slot-scope=\"scope\">\n          <el-switch\n            v-model=\"scope.row.mg_state\"\n            active-color=\"#13ce66\"\n            inactive-color=\"#ff4949\"\n            @change=\"updateStatus(scope)\"\n          >\n          </el-switch>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" icon=\"el-icon-edit\" @click=\"fillUser(scope.$index,scope.row)\" circle></el-button>\n        <el-button\n          type=\"danger\"\n          icon=\"el-icon-delete\"\n         @click=\"deleteUser(scope.$index,scope.row)\"\n          circle\n        ></el-button>\n        <el-button type=\"success\" icon=\"el-icon-check\" @click=\"currentCheck(scope.$index,scope.row)\" circle></el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n    <el-dialog\n  title=\"分配角色\"\n  :visible.sync=\"dialogVisible\"\n  width=\"30%\"\n >\n  <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"roleForm\" label-width=\"100px\" class=\"demo-ruleForm\">\n  <el-form-item label=\"当前用户\" prop=\"username\">\n    <el-input v-model=\"currentUser\"></el-input>\n  </el-form-item>\n  <el-form-item label=\"角色列表\" prop=\"roles\">\n    <el-select v-model=\"roleForm.roles\" placeholder=\"请选择角色\">\n      <el-option v-for=\"item in roleList\" :key=\"item.id\" :label=\"item.roleName\" :value=\"item.id\"></el-option>\n    </el-select>\n  </el-form-item>\n</el-form>\n  <span slot=\"footer\" class=\"dialog-footer\">\n    <el-button @click=\"dialogVisible = false\">取 消</el-button>\n    <el-button type=\"primary\" @click=\"contributeRole\" >确 定</el-button>\n  </span>\n</el-dialog>\n    \n    <el-dialog title=\"修改用户\" :visible.sync=\"updateFormVisible\" center>\n      <el-form :model=\"updateUserData\" label-position=\"top\" ref=\"updateUserForm\" class=\"demo-ruleForme\" :rules=\"updateRules\">\n        <el-form-item label=\"用户名\"  prop=\"username\" :disabled=\"true\" :label-width=\"formLabelWidth\">\n          <el-input\n            v-model=\"updateUserData.username\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"邮箱\" prop=\"email\" :label-width=\"formLabelWidth\">\n          <el-input\n            v-model=\"updateUserData.email\"\n            type=\"email\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"电话\" prop=\"mobile\" :label-width=\"formLabelWidth\">\n          <el-input\n            v-model=\"updateUserData.mobile\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"updateFormVisible = false\">取 消</el-button>\n        <el-button\n          type=\"primary\"\n          @click=\"updateUser\"\n          >确 定</el-button\n        >\n      </div>\n    </el-dialog>\n    <el-pagination\n      @size-change=\"handleSizeChange\"\n      @current-change=\"handleCurrentChange\"\n      :current-page=\"currentPage4\"\n      :page-sizes=\"[100, 200, 300, 400]\"\n      :page-size=\"userData.pagesize\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      :total=\"total\"\n    >\n    </el-pagination>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"users\",\n  data() {\n    return {\n      currentPage1: 5,\n      currentPage2: 5,\n      currentPage3: 5,\n      currentPage4: 4,\n      currentUser:\"\",\n      roleList:[],\n      dialogVisible:false,\n      dialogFormVisible: false,\n      formLabelWidth: \"120px\",\n      updateFormVisible:false,\n      id:\"\",\n      /* 更新用户信息 */\n      updateUserForm:{\n        username:\"\",\n        email:\"\",\n        mobile:\"\"\n      },\n      roleForm:{\n        roles:\"\"\n      },\n      updateRules:{\n        username:[{ required: true, message: \"请输入用户名\", trigger: \"blur\" },],\n        email:[{ required: false, message: \"请输入邮箱\", trigger: \"blur\" }],\n        mobile:[{ required: false, message: \"请输入手机号码\", trigger: \"blur\" }]\n      },\n       ruleForm: {\n          username: '',\n          password: '',\n        },\n      rules: {\n        username: [\n          { required: true, message: \"请输入用户名\", trigger: \"blur\" },\n          { min: 3, max: 5, message: \"长度在 3 到 5 个字符\", trigger: \"blur\" },\n        ],\n        password: [{ required: true, message: \"请输入密码\", trigger: \"blur\" },\n        { min: 5, max: 8, message: \"长度在 5 到 8个字符\", trigger: \"blur\" }],\n        email:[{ required: false, message: \"请输入邮箱\", trigger: \"blur\" }],\n        mobile: [\n          { min: 5, max: 11, message: \"长度在 11个字符\", trigger: \"blur\" },\n          { required: false, message: \"请输入手机号码\", trigger: \"blur\" }\n        ],\n      },\n      /* 修改用户 */\n      updateUserData:{\n        username:\"\",\n        email:\"\",\n        mobile:\"\"\n      },\n      /* 添加用户 */\n      newUserData: {\n        username: \"\",\n        password: \"\",\n        email: \"\",\n        mobile: \"\",\n      },\n      /* 修改用户状态 */\n      statusData: {\n        uId: \"\",\n        type: false,\n      },\n      /* 查询 */\n      userData: {\n        query: \"\",\n        pagenum: 1,\n        pagesize: 10,\n      },\n      total: 0,\n      tableData: [],\n    };\n  },\n  methods: {\n    handleSizeChange(val) {\n      console.log(`每页 ${val} 条`);\n    },\n    handleCurrentChange(val) {\n      console.log(`当前页: ${val}`);\n    },\n    /* 修改用户状态 */\n    updateStatus(scope) {\n      console.log(scope);\n      this.statusData.uId=scope.row.id;\n      console.log(scope.row.id)\n      this.statusData.type = scope.row.mg_state;\n      this.$request.fixStatus(this.statusData).then((res) => {\n        console.log(res);\n        if (res.data.meta.status == 200) {\n          this.$message.success(\"修改状态成功\");\n        }\n      });\n    },\n    /* 填充表单 */\n    fillUser(index,row){\n      console.log(row)\n      this.id=row.id;\n      this.updateFormVisible=true;\n      this.updateUserData.username=row.username;\n      this.updateUserData.email=row.email;\n      this.updateUserData.mobile=row.mobile;\n    },\n    /* 修改用户 */\n    updateUser(){\n      this.$request.editUser(this.updateUserData,this.id).then(res=>{\n        console.log(res)\n        if(res.data.meta.status==200){\n          this.$message.success(\"修改成功\");\n          this.getUser();\n        }\n      });\n      this.updateFormVisible = false;\n    },\n    /* 获取用户列表 */\n    getUser(){\n      this.$request.getUsers(this.userData).then((res) => {\n      if (res.data.meta.status == 200) {\n        this.total = res.data.data.total;\n        this.tableData = res.data.data.users;\n      }\n    });\n    },\n    /* 删除用户 */\n    deleteUser(index,row) {\n      console.log(row);\n      this.$request\n        .deleteUser({\n          id: row.id\n        })\n        .then((res) => {\n          console.log(res);\n          if (res.data.meta.status == 200) {\n            this.$message.success(\"删除成功\");\n            this.getUser();\n          }\n        });\n    },\n    /* 添加用户 */\n    addUser() {\n      this.dialogFormVisible = false\n      console.log(this.newUserData);\n      console.log(1);\n      this.$request.addUser(this.newUserData).then((res) => {\n        console.log(res);\n        if (res.data.meta.status == 201) {\n          this.$message.success(\"添加用户成功\");\n          this.getUser();\n        }\n      });\n    },\n    /* 查询用户 */\n    search() {\n      console.log(1);\n      if(this.userData.query){\n      this.$request.searchUser({id: this.userData.query}).then((res) => {\n          if (res.data.meta.status == 200) {\n            var arr=[];\n            arr.push(res.data.data);\n            this.tableData = arr;\n          }\n        });\n      }else{\n        this.getUser();\n      }\n    },\n    currentCheck(index,row){\n     this.currentUser=row.username;\n     this.dialogVisible=true;\n     this.id=row.id;\n   },\n    contributeRole(){\n      console.log(this.roleForm.roles);\n      this.$request.editRole(this.id,this.roleForm.roles).then(res=>{\n        console.log(res);\n        if(res.data.meta.status==200){\n          this.$message.success(\"修改成功\");\n        }\n      });\n      this.dialogVisible = false;\n    }\n  },\n  created() {\n   this.getUser();\n   this.$request.roleList().then(res=>{\n     console.log(res);\n     if(res.data.meta.status==200){\n       this.$message.success(\"获取列表成功\");\n       this.roleList=res.data.data;\n     }\n   });\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n#users {\n  .breadcrumb {\n    height: 30px;\n  }\n}\n</style>"],"sourceRoot":"src/components"}]}